import React from 'react'
import {BrowserRouter,Route} from 'react-router-dom' 
import Home from './Home'
import Eligibility from './eligibility' ;
import Compability  from './compability'
import Bloodfacts  from './Bloodfacts'
import Register  from './donarregister'
import Searchdonar  from './searchdonar'
import Header from './Header'
import Footer from './Footer'
import Navbar from './Nav'


const Routing = () => {
    return(
       
            <BrowserRouter>
             <div>
                <Navbar/>
                <Header/> 
                <Route exact path='/' component={Home}/>
                <Route path ='/ELIGIBILITY' component={Eligibility}/>
                <Route path ='/Compability' component={Compability}/>
                <Route path ='/Bloodfacts' component={Bloodfacts}/>
                <Route path ='/Donarregister' component={Register}/>
                <Route path ='/Searchdonar' component={Searchdonar}/>
                <Footer/>
            </div>
            </BrowserRouter>
        
    )
}

export default Routing